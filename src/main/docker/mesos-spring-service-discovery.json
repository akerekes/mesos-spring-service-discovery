{
    "id": "/spring-service-discovery-2", 
    "cpus": 1,
    "mem": 2048,
    "instances": 1,
    "container": {
      "type": "DOCKER",
      "docker": {
        "network": "BRIDGE",
        "portMappings": [
          { "containerPort": 8080, "protocol": "tcp" }
        ],
        "image": "akerekes/mesos-spring-service-discovery",
        "forcePullImage": true
      }
    },
    "cmd": "java -jar /service-discovery-demo-0.0.1-SNAPSHOT.jar --spring.config.name=randomnumber-application --eureka.client.serviceUrl=http://eureka-server.marathon.mesos:8761" 
}
